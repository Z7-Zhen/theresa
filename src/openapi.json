        "/image/ba": {
            "get": {
                "summary": "Blue Archive",
                "description": "Blue Archive Random Images",
                "tags": [
                    "Image"
                ],
                "responses": {
                    "200": {
                        "description": "Respon berhasil",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "images": {
                                            "type": "array",
                                            "items": {
                                                "type": "string",
                                                "format": "uri"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Gagal memproses request"
                    }
                }
            }
        },
        "/info/manwha": {
