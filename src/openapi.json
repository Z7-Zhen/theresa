        "/search/loker": {
            "get": {
                "summary": "JobStreet Loker Finder",
                "description": "Cari lowongan pekerjaan berdasarkan kata kunci dan kota",
                "tags": [
                    "Search"
                ],
                "parameters": [
                    {
                        "name": "pekerjaan",
                        "in": "query",
                        "required": true,
                        "description": "Kata kunci pekerjaan",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "kota",
                        "in": "query",
                        "required": true,
                        "description": "Lokasi / kota",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "jumlah",
                        "in": "query",
                        "required": false,
                        "description": "Jumlah hasil (default 5)",
                        "schema": {
                            "type": "integer",
                            "default": 5
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Respon berhasil",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "creator": {
                                            "type": "string",
                                            "example": "Z7:林企业"
                                        },
                                        "result": {
                                            "type": "object",
                                            "properties": {
                                                "query": {
                                                    "type": "object",
                                                    "properties": {
                                                        "pekerjaan": {
                                                            "type": "string",
                                                            "example": "Barista"
                                                        },
                                                        "kota": {
                                                            "type": "string",
                                                            "example": "Bandung"
                                                        },
                                                        "jumlah": {
                                                            "type": "integer",
                                                            "example": 5
                                                        }
                                                    }
                                                },
                                                "total": {
                                                    "type": "integer",
                                                    "example": 5
                                                },
                                                "jobs": {
                                                    "type": "array",
                                                    "items": {
                                                        "type": "object",
                                                        "properties": {
                                                            "title": {
                                                                "type": "string",
                                                                "example": "Barista"
                                                            },
                                                            "company": {
                                                                "type": "string",
                                                                "example": "Starbucks"
                                                            },
                                                            "location": {
                                                                "type": "string",
                                                                "example": "Bandung"
                                                            },
                                                            "date": {
                                                                "type": "string",
                                                                "example": "16 Nov 2025"
                                                            },
                                                            "salary": {
                                                                "type": "string",
                                                                "example": "Tidak dicantumkan"
                                                            },
                                                            "description": {
                                                                "type": "string",
                                                                "example": "Job teaser"
                                                            },
                                                            "logo": {
                                                                "type": "string",
                                                                "format": "uri"
                                                            },
                                                            "link": {
                                                                "type": "string",
                                                                "format": "uri"
                                                            }
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Gagal memproses request",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "type": "boolean",
                                            "example": false
                                        },
                                        "creator": {
                                            "type": "string",
                                            "example": "Z7:林企业"
                                        },
                                        "error": {
                                            "type": "string",
                                            "example": "Tidak ada lowongan ditemukan"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
