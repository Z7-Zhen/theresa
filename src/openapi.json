                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "creator": {
                                            "type": "string",
                                            "example": "Z7:林企业"
                                        },
                                        "result": {
                                            "type": "object",
                                            "properties": {
                                                "updated": {
                                                    "type": "string",
                                                    "format": "date-time"
                                                },
                                                "user": {
                                                    "type": "object",
                                                    "properties": {
                                                        "playerName": {
                                                            "type": "string",
                                                            "example": "Player123"
                                                        },
                                                        "userId": {
                                                            "type": "string",
                                                            "example": "987654321"
                                                        },
                                                        "sessionId": {
                                                            "type": "string",
                                                            "example": "abc123xyz"
                                                        }
                                                    }
                                                },
                                                "garden": {
                                                    "type": "object",
                                                    "properties": {
                                                        "updateNumber": {
                                                            "type": "integer",
                                                            "example": 5
                                                        },
                                                        "timestamp": {
                                                            "type": "string",
                                                            "example": "2025-11-16T12:00:00Z"
                                                        },
                                                        "weather": {
                                                            "type": "string",
                                                            "example": "Sunny"
                                                        },
                                                        "seeds": {
                                                            "type": "array",
                                                            "items": {
                                                                "type": "object",
                                                                "properties": {
                                                                    "name": {
                                                                        "type": "string",
                                                                        "example": "Carrot"
                                                                    },
                                                                    "quantity": {
                                                                        "type": "integer",
                                                                        "example": 10
                                                                    },
                                                                    "image": {
                                                                        "type": "string",
                                                                        "format": "uri"
                                                                    }
                                                                }
                                                            }
                                                        },
                                                        "gear": {
                                                            "type": "array",
                                                            "items": {
                                                                "type": "object",
                                                                "properties": {
                                                                    "name": {
                                                                        "type": "string",
                                                                        "example": "Shovel"
                                                                    },
                                                                    "quantity": {
                                                                        "type": "integer",
                                                                        "example": 1
                                                                    },
                                                                    "image": {
                                                                        "type": "string",
                                                                        "format": "uri"
                                                                    }
                                                                }
                                                            }
                                                        },
                                                        "cosmetic": {
                                                            "type": "array",
                                                            "items": {
                                                                "type": "object",
                                                                "properties": {
                                                                    "name": {
                                                                        "type": "string",
                                                                        "example": "Hat"
                                                                    },
                                                                    "quantity": {
                                                                        "type": "integer",
                                                                        "example": 1
                                                                    },
                                                                    "image": {
                                                                        "type": "string",
                                                                        "format": "uri"
                                                                    }
                                                                }
                                                            }
                                                        },
                                                        "eggs": {
                                                            "type": "array",
                                                            "items": {
                                                                "type": "object",
                                                                "properties": {
                                                                    "name": {
                                                                        "type": "string",
                                                                        "example": "Golden Egg"
                                                                    },
                                                                    "quantity": {
                                                                        "type": "integer",
                                                                        "example": 2
                                                                    },
                                                                    "image": {
                                                                        "type": "string",
                                                                        "format": "uri"
                                                                    }
                                                                }
                                                            }
                                                        },
                                                        "event": {
                                                            "type": "object"
                                                        },
                                                        "honeyevent": {
                                                            "type": "object"
                                                        },
                                                        "nightevent": {
                                                            "type": "object"
                                                        },
                                                        "traveling": {
                                                            "type": "object"
                                                        }
                                                    }
                                                },
                                                "meta": {
                                                    "type": "object"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Terjadi kesalahan server",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "type": "boolean",
                                            "example": false
                                        },
                                        "creator": {
                                            "type": "string",
                                            "example": "Z7:林企业"
                                        },
                                        "error": {
                                            "type": "string",
                                            "example": "Gagal mengambil data Grow a Garden"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/download/soundcloud": {
            "get": {
                "summary": "SoundCloud Downloader",
                "description": "Download lagu dari SoundCloud",
                "tags": [
                    "Downloader"
                ],
                "parameters": [
                    {
                        "name": "url",
                        "in": "query",
                        "required": true,
                        "description": "URL lagu SoundCloud yang ingin diunduh",
                        "schema": {
                            "type": "string",
                            "format": "uri"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Download berhasil",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "creator": {
                                            "type": "string",
                                            "example": "Z7:林企业"
                                        },
                                        "result": {
                                            "type": "object"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "URL tidak valid atau kosong"
                    },
                    "500": {
                        "description": "Gagal memproses request"
                    }
                }
            }
        },
